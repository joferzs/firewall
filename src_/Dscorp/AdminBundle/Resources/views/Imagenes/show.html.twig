{% extends '::base.html.twig' %} 
{% block title %}Admin Imagenes{% endblock %}
{% block contenido %}
<h1>Imagenes</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombreimagen</th>
            <td>{{ entity.nombreImagen }}</td>
        </tr>
        <tr>
            <th>Path</th>
            <td>{{ entity.path }}</td>
        </tr>
        <tr>
            <th>Imagen</th>
            <td>{{ entity.imagen }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('imagenes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('imagenes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('imagenes_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}