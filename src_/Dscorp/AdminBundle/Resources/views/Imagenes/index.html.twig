{% extends '::base.html.twig' %} 
{% block title %}Admin Imagenes{% endblock %}
{% block contenido %}
<h1>Imagenes list</h1>

<table class="table table-striped table-bordered table-hover dataTables-example">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombreimagen</th>
            <th>Path</th>
            <th>Imagen</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('imagenes_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombreImagen }}</td>
            <td>{{ entity.path }}</td>
            <td>{{ entity.imagen }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('imagenes_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('imagenes_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('imagenes_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}