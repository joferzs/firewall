{% extends '::basevista.html.twig' %} 
{% block title %}Vista{% endblock %}
{% block contenido %}
<div class="page ng-scope">
    <div class="panel panel-default">
        <div class="panel-heading"><strong><span class="glyphicon glyphicon-th"></span> Pruebas</strong></div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6">
                    {% if entities|length > 0 %}
                    {% for entity in entities %}
                    <li>            
                        <span data-translate="{{entity.nombrecontenido}}" class="ng-scope">UI Kit</span>                        
                    </li>
                    {% endfor %}
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}
