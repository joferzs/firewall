{% extends '::base.html.twig' %} 
{% block title %}Admin Tema{% endblock %}
{% block contenido %}
<h1>Tema list</h1>

<table class="table table-striped table-bordered table-hover dataTables-example">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombretema</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('tema_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombreTema }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('tema_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('tema_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('tema_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

{% endblock %}