{% extends '::base.html.twig' %} 
{% block title %}Admin Cotenido{% endblock %}
{% block contenido %}
<h1>Contenido list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombrecontenido</th>
            <th>NombreSubtema</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('contenido_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombreContenido }}</td>
            <td>{{ entity.subtema }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('contenido_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('contenido_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('contenido_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

{% endblock %}