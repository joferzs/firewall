{% extends '::base.html.twig' %} 
{% block title %}Admin Cotenido{% endblock %}
{% block contenido %}
<h1>Contenido</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombrecontenido</th>
            <td>{{ entity.nombreContenido }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('contenido') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contenido_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('contenido_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

{% endblock %}