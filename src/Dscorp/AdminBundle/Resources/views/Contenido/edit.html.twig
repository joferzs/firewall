{% extends '::base.html.twig' %} 
{% block title %}Admin Cotenido{% endblock %}
{% block contenido %}
<h1>Contenido edit</h1>

<form action="{{ path('contenido_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('contenido') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('contenido_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>

{% endblock %}
