                    <ul class="nav-right pull-right list-unstyled">
                        <li class="dropdown text-normal nav-profile" dropdown="" is-open="status.isopenProfile">
                            <a href="javascript:;" dropdown-toggle="" ng-disabled="disabled" aria-haspopup="true" aria-expanded="false">
                                <img src="images/g1.jpg" alt="" class="img-circle img30_30">
                                <span class="hidden-xs">
                                    <span data-translate="LISA" class="ng-scope">Lisa</span>
                                </span>
                            </a>
                            <ul class="dropdown-menu with-arrow pull-right">
                                <li>
                                    <a href="#/pages/profile">
                                        <i class="ti-user"></i>
                                        <span data-translate="MY_PROFILE" class="ng-scope">My Profile</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#/app/tasks">
                                        <i class="ti-check-box"></i>
                                        <span data-translate="MY_TASKS" class="ng-scope">My Tasks</span>
                                    </a>
                                </li>
                                <li>
